<div class="page-title clearfix">
  <h1>Complexes</h1>
  {{#link-to 'rental-complexes.new' class="btn btn-primary"}}Add{{/link-to}}
</div>
<div class="border-block">
<table class="table table-striped table-hover table-bordered">
  <thead>
    <th>Name</th>
    <th>Addesss</th>
    <th>customer Contact Email Address</th>
    <th>Action</th>
  </thead>
  <tbody>
    {{#each model}}
      <tr>
        <td>
          <div>{{name}}</div>
        </td>
        <td>
          <div>{{fullAddress}}</div>
        </td>
        <td>
          <div>{{customerContactEmailAddress}}</div>
        </td>
        <td>
          {{#link-to 'rental-complexes.show' id class="btn btn-primary"}}show{{/link-to}}
          {{#link-to 'rental-complexes.edit' id class="btn btn-primary"}}Edit{{/link-to}}
          <button {{action "deleteRecord" this}} class="btn btn-danger"> Delete</button>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
				<div class="row">
					<div class="col-xs-12 col-sm-8 col-md-8 form-group">
{{page-numbers content=content}}
					</div>
					<div class="col-xs-12 col-sm-4 col-md-4 form-group">
{{#link-to 'rental-offerings.index' class="btn btn-primary right btn-padding-top"}}Rental Offering{{/link-to}}
					</div>
				</div>
</div>
