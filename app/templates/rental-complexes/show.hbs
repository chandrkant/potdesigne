<div class="page-title clearfix">
    <h1>Complex</h1>
</div>
<div class="border-block">  
  <table class="table table-striped table-hover">
     <tr class="active">
        <td><b>Name</b></td>
        <td> {{name}} </td>
     </tr>
     <tr class="active">
        <td><b> Full Address</b></td>
        <td> {{fullAddress}} </td>
     </tr>
     <tr class="active">
        <td><b>Customer Contact Email Address</b></td>
        <td> {{customerContactEmailAddress}} </td>
     </tr>
     <tr class="active">
        <td><b>Customer Contact Phone Number</b></td>
        <td>{{customerContactPhoneNumber}}</td>
     </tr>
     <tr class="active">
        <td><b>Description</b></td>
        <td>{{salesyDescription}}</td>
     </tr>
  </table>
</div>
<br>
<div>
   {{#link-to "rental-complexes.edit" this.id class="btn btn-primary"}}Edit{{/link-to}}
   <button class="btn btn-default" {{action 'cancel'}} >Cancel</button>
</div>
