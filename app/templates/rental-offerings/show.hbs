        <div class="page-title clearfix">
            <h1>Properties Listing</h1>
        </div>

<div class="border-block">
    <table class="table table-striped table-hover">
       <tr class="active">
          <td><b>Headline</b></td>
          <td> {{headline}} </td>
       </tr>
       <tr class="active">
          <td><b>URL</b></td>
          <td> {{url}} </td>
       </tr>
       <tr class="active">
          <td><b>Available On</b></td>
          <td> {{earliestAvailableOn}} </td>
       </tr>
       <tr class="active">
          <td><b>Bedroom Count</b></td>
          <td>{{bedroomCount}}</td>
       </tr>
       <tr class="active">
          <td><b>Full Bathrooom Count</b></td>
          <td>{{fullBathroomCount}}</td>
       </tr>
       <tr class="active">
          <td><b>Half Bathroom Count</b></td>
          <td> {{halfBathroomCount}} </td>
       </tr>
       <tr class="active">
          <td><b>Monthly Rent Floor</b></td>
          <td>{{monthlyRentFloor}}</td>
       </tr>
       <tr class="active">
          <td><b>Monthly Rent Ceiling</b></td>
          <td>{{monthlyRentCeiling}}</td>
       </tr>
       <tr class="active">
          <td><b>Square Footage Floor</b></td>
          <td>{{squareFootageFloor}}</td>
       </tr>
       <tr class="active">
          <td><b>Square Footage Ceiling</b></td>
          <td>{{squareFootageCeiling}}</td>
       </tr>
       <tr class="active">
          <td><b>Rental Offering Type</b></td>
          <td>{{rentalOfferingType}}</td>
       </tr>
       <tr class="active">
          <td><b>Description</b></td>
          <td>{{salesyDescription}}</td>
       </tr>
    </table>
</div>
<!--product images gallery start-->
<div class="panel panel-primary pure-u-1">
    <div class="panel-heading">
      <h3 class="panel-title">Property Photos</h3>
    </div>
  <div class="panel-body">
       <div class="row">
          <div class="col-md-6">
            <div class="slider slider-nav">
              {{#each photos}}
                <div style="float:left; margin-right:15px;">
                  <img src="{{unbound thumb}}"/>
                </div>
              {{/each}}
            </div>
          </div>
        </div>
  </div>
</div>
<br>
<div>
   {{#link-to "rental-offerings.edit" this.id class="btn btn-primary"}}Edit{{/link-to}}
   <button class="btn btn-default" {{action 'cancel'}} >Cancel</button>
</div>
<br>
